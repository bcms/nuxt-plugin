{
  "name": "nuxt-plugin-bcms",
  "version": "3.1.2",
  "description": "Nuxt plugin for BCMS",
  "main": "./module.js",
  "types": "./index.d.ts",
  "scripts": {
    "build:ts": "tsc -p .",
    "build:watch": "tsc -w",
    "bundle": "npm-tool --bundle",
    "pack": "npm-tool --pack",
    "publish": "npm-tool --publish",
    "build:components": "vue-cli-service build --target lib --name components-library ./components/index.ts --dest dist/components"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/becomesco/nuxt-plugin-bcms.git"
  },
  "author": "becomes.co",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/becomesco/nuxt-plugin-bcms/issues"
  },
  "keywords": [
    "nuxt",
    "vue",
    "plugin",
    "bcms"
  ],
  "homepage": "https://github.com/becomesco/nuxt-plugin-bcms#readme",
  "devDependencies": {
    "@banez/child_process": "^1.0.3",
    "@banez/fs": "^1.0.3",
    "@banez/npm-tool": "^1.1.1",
    "@becomes/cms-cli": "^3.1.0",
    "@becomes/cms-client": "^3.3.5",
    "@becomes/cms-most": "^2.7.0",
    "@nuxt/kit": "^3.0.0",
    "@types/node": "^20.3.2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.43.0",
    "h3": "^1.7.1",
    "nuxt": "^3.6.1",
    "prettier": "^2.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vue": "^3.3.4"
  },
  "peerDependencies": {
    "@becomes/cms-client": ">=3.3.5",
    "@becomes/cms-most": ">=2.7.0",
    "nuxt": "^3.0.0"
  },
  "dependencies": {
    "axios": "^0.27.2"
  }
}
